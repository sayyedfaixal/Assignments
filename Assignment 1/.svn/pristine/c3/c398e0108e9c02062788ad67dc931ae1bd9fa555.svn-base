/* importing google fonts */
@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap');
*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
}

body{
    background: #353F8C;
}
.navbar{
    background-color: #fff;
    border-radius: 5px;
}

.logo{
    float: left;
    height: 5rem;
    width: 5rem;
    padding-bottom: 10px;
    padding-left: 5px;
}

.name{
    color: #FF3263;
    font-size: 50px;
    font-weight: 600;
}
.heading{
    color: #fff;
    font-weight: 500;
    padding-top: 2%;
    padding-left: 2%;
}
a{
    display: block;
    color: #353F8C;
    text-decoration: none;
}
.boy{
    position: absolute;
